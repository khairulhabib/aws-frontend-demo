extends layout

block content
  h1= title
  p Welcome to monitoring room 
  form(name="increase",method="post")
    .actions 
      input(type="hidden",name="control",value="increase")
      button.btn.btn-2.btn-sep.icon-send(type="submit",name="increase") Increase
  form(name="decrease",method="post")
    .actions 
      input(type="hidden",name="control",value="decrease")
      button.btn.btn-3.btn-sep.icon-minus(type="submit",value="decrease")
